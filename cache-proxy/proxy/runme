DATE=$(date)
PWD=$(pwd)

echo $DATE

source ~/.profile
npm config del registry;npm config del proxy;npm config del https-proxy

server=$(which npm-proxy-cache)
[ -z $server ] && npm install -g npm-proxy-cache

npm set ca null;npm config set proxy http://localhost:8080;npm config set https-proxy http://localhost:8080;npm config set strict-ssl false
mkdir `pwd`/cache
npm-proxy-cache --storage `pwd`/cache

npm config del registry;npm config del proxy;npm config del https-proxy
